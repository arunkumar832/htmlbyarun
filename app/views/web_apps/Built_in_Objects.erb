<script>

  function EmiCal(){
     loanAmt = document.getElementById("loanAmt").value;
     noMonths = document.getElementById("noMonths").value;
     intr = document.getElementById("intr").value;
     var pay = document.getElementById("pay");
    if((loanAmt.length == "" && noMonths.length == "" && intr.length == "")||(loanAmt.length == "" || noMonths.length == "" || intr.length == ""))
    {
      pay.style.color = "red";
      pay.style.fontWeight = "bold";
      pay.value = 'Please Enter Valid inputs';
    }
    else
    {
      pay.style.color = "blue";
      pay.style.fontWeight = "bold";
      var paym = loanAmt * intr / (1-(Math.pow(1/(1+intr),noMonths)));
      if(paym % 1 == 0)
        var emiout=parseInt(paym);
      else
        emiout = paym.toFixed(4);
      pay.value  = emiout;
    }

  }
</script>
